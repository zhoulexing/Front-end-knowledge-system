{
    "presets": [
        ["@babel/preset-env", {
            // "useBuiltIns": "usage",
            // "corejs": 3,
            "targets": {
                "browsers": ["> 0.25%, not dead", "last 2 versions"]
            }
        }]
    ],
    "plugins": [
        "@babel/plugin-syntax-top-level-await",
        ["@babel/plugin-transform-runtime", {
            "corejs": 3,
            "regenerator": true,
            "helpers": true,
            "absoluteRuntime": false,
            "useESModules": false
        }],
        "@babel/plugin-proposal-private-methods"
    ]
}