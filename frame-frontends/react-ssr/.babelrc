{
    "presets": [
        "@babel/preset-react"
    ],
    "env": {
        "development": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "targets": {
                            "browsers": "> 0.25%, not dead"
                        }
                    }
                ]
            ],
            "plugins": [
                "@babel/plugin-transform-runtime"
            ]
        },
        "node": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "targets":{
                            "node":"current"
                        }
                    }
                ]
            ]
        }
    }
}
