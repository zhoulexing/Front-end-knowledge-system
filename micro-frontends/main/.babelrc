{
    "presets": [
        "@babel/preset-react",
        [
            "@babel/preset-env",
            {
                "targets": { "browsers": "> 0.25%, not dead" }
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "corejs": 3,
                "regenerator": true,
                "helpers": true,
                "useESModules": true
            }
        ]
    ]
}
